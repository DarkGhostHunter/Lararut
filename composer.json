{
    "name": "darkghosthunter/lararut",
    "description": "RutUtils integration for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Italo Baeza C.",
            "email": "DarkGhostHunter@Gmail.com"
        }
    ],
    "support": {
        "source": "https://github.com/darkghosthunter/lararut"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4||^8.0",
        "illuminate/validation": "^7.0||^8.0",
        "illuminate/support": "^7.0||^8.0",
        "darkghosthunter/rut-utils": "^3.0.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0||^6.0",
        "phpunit/phpunit": "^9.3",
        "mockery/mockery": "^1.4",
        "doctrine/dbal": "^2.8 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "DarkGhostHunter\\Lararut\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --coverage-clover build/logs/clover.xml",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DarkGhostHunter\\Lararut\\LararutServiceProvider"
            ],
            "aliases": {
                "Rut": "DarkGhostHunter\\Lararut\\Facades\\Rut"
            }
        }
    }
}
